{
  "name": "@l1xnan/tree-sitter-sql",
  "version": "0.4.1",
  "description": "Tree-sitter Grammar for SQL",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "generate": "tree-sitter generate",
    "install": "tree-sitter generate && node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "npx --yes --package=tree-sitter-cli@v0.25.3 -- tree-sitter playground",
    "playground": "npm run install && npm run prestart && npm run start",
    "release": "commit-and-tag-version",
    "test": "node --test bindings/node/*_test.js",
    "prebuildify": "prebuildify --napi --strip"
  },
  "author": "derek stride",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "commit-and-tag-version": "^12.0.0",
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.24.7"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "gypfile": true,
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derekstride/tree-sitter-sql.git"
  },
  "bugs": {
    "url": "https://github.com/derekstride/tree-sitter-sql/issues"
  },
  "homepage": "https://github.com/derekstride/tree-sitter-sql#readme",
  "commit-and-tag-version": {
    "skip": {
      "tag": true
    }
  },
  "keywords": [
    "parser",
    "sql"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "publishConfig": {
    "access": "public"
  }
}
